#include <bits/stdc++.h> 
using namespace std;

int main() {
    int t;
    cin>>t;
    while(t--) {
        string str;
        cin>>str;
        int counter = 0;
        for(int i = 0; i < (int)str.length(); i++) {
            if(isupper(str[i])) counter++;
        } 
        if(counter <= 6) cout<<"YES\n";
        else cout<<"NO\n";
    }




    return 0;
}
